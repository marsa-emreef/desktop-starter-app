{
  "name": "esnaadm",
  "version": "1.2.2",
  "description": "EsnaadM Application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "ae.mil.jac.esnaadm",
    "files": [
      "!**/project/*"
    ],
    "extraResources": [
      "./*.dll",
      {
        "from": "./project/bin-debug/",
        "to": "app",
        "filter": [
          "**/*"
        ]
      }
    ],
    "nsis": {
      "perMachine": false,
      "allowToChangeInstallationDirectory": false,
      "allowElevation": false,
      "oneClick": false
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "author": "Global Aerospace Logistic, LLC",
  "license": "ISC",
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-builder": "^20.39.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.4",
    "multer": "^1.4.1",
    "unzip": "^0.1.11"
  }
}
